{
  "fuseVersion": "1.8.12",
  "schedule": {
    "cron": "*/1 * * * *",
    "timezone": "US/Pacific"
  },
  "security": {
    "functionPermissions": {
      "allow": [
        {
          "action": "storage:*",
          "resource": "/account/{{accountId}}/subscription/{{subscriptionId}}/storage/boundary/{{boundaryId}}/function/{{functionId}}/"
        }
      ]
    }
  }
}